# Database Configuration
# Set DB_TYPE to "sqlite" for local development or "postgres" for production
DB_TYPE=sqlite

# PostgreSQL Connection String (required if DB_TYPE is "postgres")
# Format: postgresql://username:password@host:port/database
POSTGRES_CONNECTION_STRING=postgresql://postgres:password@localhost:5432/chatapp

# API Keys for LLM Providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Application Settings
LOG_LEVEL=INFO
CORS_ORIGINS=http://localhost:9000
